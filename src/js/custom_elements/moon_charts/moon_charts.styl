moon-charts
  animation scaleIn transition-intro ease
  display block
  grid-area charts
  margin auto
  max-width chart-max-size
  padding 0 0 100%
  position relative
  width 100%

  [no-js] &
    display none

  svg
    height 100%
    position absolute
    width 100%

  [id=axes] line
    stroke black

  [id=ticks] line
    stroke rgba(white, .7)
